{
  "private": true,
  "name": "gunrock",
  "version": "1.0.0",
  "description": "Professional website for UC Davis ARE PhD candidates",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:smythian/gunrock.git"
  },
  "author": "Eric Smyth <https://smyth.app>",
  "license": "MIT",
  "main": "n/a",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "start": "testing/start-dev-server.sh",
    "clean": "rimraf .cache public coverage",
    "build": "gatsby build",
    "serve": "gatsby serve --port 8001 --open",
    "lint": "yarn lint:yaml && yarn lint:md && yarn lint:js",
    "lint:js": "eslint .",
    "lint:md": "remark src/pages",
    "lint:yaml": "yamllint **/*.yml --ignore=node_modules/**/*.yml",
    "test": "yarn test:unit && yarn test:visual",
    "test:unit": "cross-env NODE_ENV=test jest --coverage",
    "test:visual": "testing/run-visual-regression-test.sh",
    "test:visual:ci": "cross-env NODE_ENV=test jest image-storyshots.runner.js --testRegex \".*\\.runner\\.js$\"",
    "storybook": "testing/start-storybook.sh",
    "storybook:build": "build-storybook",
    "security": "yarn audit",
    "snyk-protect": "snyk protect",
    "postinstall": "hash=`sha256sum yarn.lock | awk '{ print $1 }'` && sed -i \"s/key:.*$/key: ${hash}/g\" .gitlab-ci.yml"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.5",
    "@fortawesome/free-brands-svg-icons": "^5.4.0",
    "@fortawesome/free-regular-svg-icons": "^5.4.0",
    "@fortawesome/free-solid-svg-icons": "^5.4.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@material-ui/core": "^3.2.0",
    "gatsby": "^2.0.0",
    "gatsby-plugin-catch-links": "^2.0.4",
    "gatsby-plugin-manifest": "^2.0.7",
    "gatsby-plugin-netlify": "^2.0.0",
    "gatsby-plugin-netlify-cms": "^3.0.0",
    "gatsby-plugin-offline": "^2.0.12",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.7",
    "gatsby-plugin-sitemap": "^2.0.1",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-copy-images": "^0.2.1",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^2.0.4",
    "gatsby-remark-katex": "^2.0.5",
    "gatsby-remark-prismjs": "^3.0.1",
    "gatsby-remark-relative-images": "^0.2.0",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-json": "^2.1.5",
    "gatsby-transformer-remark": "^2.1.7",
    "gatsby-transformer-sharp": "^2.1.1",
    "katex": "^0.10.0",
    "lodash": "^4.17.5",
    "netlify-cms": "^2.1.1",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.0",
    "react": "^16.3.0",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "rehype-react": "^3.0.3",
    "sanitize.css": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "^4.0.2",
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-links": "^4.0.2",
    "@storybook/addon-options": "^4.0.2",
    "@storybook/addon-storyshots": "^4.0.2",
    "@storybook/addon-storyshots-puppeteer": "^4.0.2",
    "@storybook/addon-viewport": "^4.0.2",
    "@storybook/addons": "^4.0.2",
    "@storybook/cli": "^4.0.2",
    "@storybook/components": "^4.0.2",
    "@storybook/react": "^4.0.2",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-remove-graphql-queries": "^2.5.1",
    "babel-plugin-require-context-hook": "^1.0.0",
    "content-security-policy-builder": "^2.0.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^1.1.1",
    "imports-loader": "^0.8.0",
    "jest": "^23.6.0",
    "jss": "^9.8.7",
    "jss-preset-default": "^4.5.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^4.3.11",
    "react-jss": "^8.6.1",
    "react-storybook-addon-props-combinations": "^1.1.0",
    "remark-cli": "^6.0.0",
    "remark-frontmatter": "^1.3.0",
    "remark-preset-lint-recommended": "^3.0.2",
    "rimraf": "^2.6.2",
    "snyk": "^1.108.0",
    "yaml-lint": "^1.2.4"
  },
  "snyk": true
}
